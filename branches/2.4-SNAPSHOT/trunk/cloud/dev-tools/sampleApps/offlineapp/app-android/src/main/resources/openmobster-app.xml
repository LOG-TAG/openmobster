<app-conf>
	<encryption>false</encryption>
	
	<push>
		<launch-activity-class>com.offlineApp.android.app.screen.HomeScreen</launch-activity-class>
		<icon-name>push</icon-name>
		<!-- enable background re-establishment of the Push Socket if its disconnected -->
		<reconnect/>
	</push>
	
	<channels>
		<channel name='offlineapp_demochannel'>
			<sync-push-message>You have {0} Demo Channel Update(s)</sync-push-message>
		</channel>
	</channels>
</app-conf>