<?xml version="1.0" encoding="UTF-8"?>

<deployment xmlns="urn:jboss:bean-deployer:2.0">
   <bean name="/mysqlapp/HibernateManager" class="org.openmobster.core.common.database.HibernateManager">
   		<property name="config">mysqlapp.cfg.xml</property>
   </bean>
   
   <bean name="data_object_channel" class="com.mysqlapp.cloud.DataObjectChannel">
   		<property name="hibernateManager">
   	  		<inject bean="/mysqlapp/HibernateManager"/>
   		</property>
   		<property name="deviceController">
   	  		<inject bean="security://DeviceController"/>
   		</property>
   </bean>
</deployment>